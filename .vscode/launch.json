  "version": "0.2.0",

  "configurations": [
    {
      "name": "GenerateGraph",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "generate-graph"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "name": "AcquireHubs",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "acquire-hubs"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node",
      "cwd": "${workspaceFolder}//acquire"
    },
    {
      "name": "GenerateVisualisation",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "generate-visualisation"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "name": "GenerateTxData",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "generate-graph"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
  ]
